<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Identity-Scenario-Results">
    <parameter name="useDefaultListeners" value="false"/>
    <listeners>
        <listener class-name="org.wso2.identity.scenarios.commons.listeners.IdentityScenarioTestListener"/>
    </listeners>

    <test name="is-single-sign-on" preserve-order="true" parallel="false">
        <classes>
            <class name="org.wso2.identity.scenarios.access.delegation.oauth2.implicit.OAuth2ImplicitGrantTest"/>
        </classes>
    </test>
</suite>
