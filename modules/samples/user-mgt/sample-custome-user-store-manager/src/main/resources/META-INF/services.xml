<serviceGroup>
    <service name="CustomUserStoreManager" scope="transportsession">
        <transports>
            <transport>https</transport>
        </transports>
        <config name="ServiceClass" locked="false">
            org.wso2.sample.user.store.manager.CustomUserStoreManager
        </config>
        <config name="DoAuthentication" locked="true">false</config>
        <config name="DoAuthorization" locked="true">false</config>
        <excludeOperations>
            <operation>isHandle</operation>
            <operation>isAuthenticated</operation>
            <operation>getPriority</operation>
            <operation>getAuthenticatorName</operation>
            <operation>isDisabled</operation>
        </excludeOperations>
    </service>
    <config name="adminService" locked="true">true</config>
    <config name="hiddenService" locked="true">true</config>
</serviceGroup>
