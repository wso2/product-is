# Google reCAPTCHA settings

# Enable Google reCAPTCHA
recaptcha.enabled={{identity.recaptcha.enabled}}

# reCaptcha API URL
recaptcha.api.url={{identity.recaptcha.api_url}}

# reCaptcha verification URL
recaptcha.verify.url={{identity.recaptcha.verify_url}}

# reCaptcha site key
recaptcha.site.key={{identity.recaptcha.site_key}}

# reCaptcha secret key
recaptcha.secret.key={{identity.recaptcha.secret_key}}

# reCaptcha failed redirect urls
#recaptcha.failed.redirect.urls=
